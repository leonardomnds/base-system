{
  "name": "basex",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.15.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npm run release && next start -p $PORT",
    "migrate": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
    "release": "npx prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "postinstall": "npm run generate && npm run build",
    "heroku-postbuild": "npm run generate && npm run release && npm run build"
  },
  "browser": {
    "child_process": false
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@prisma/client": "^2.20.1",
    "@react-pdf/renderer": "^2.0.4",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "date-fns": "^2.20.1",
    "ejs": "^3.1.6",
    "html-pdf": "^2.2.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "next": "10.1.2",
    "next-cookie": "^2.6.0",
    "nextjs-cors": "^1.0.5",
    "nprogress": "^0.2.0",
    "prisma": "^2.20.1",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toast-notifications": "^2.4.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/ejs": "^3.0.6",
    "@types/html-pdf": "^2.2.0",
    "typescript": "^4.2.3"
  }
}
